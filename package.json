{
  "name": "backbone-forms",
  "version": "0.1.1",
  "description": "Render forms based on schema definition",
  "main": "forms.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha test",
    "build": "browserify examples/main.js -o examples/bundled.js"
  },
  "browserify-shim": {
    "jquery": "global:$"
  },
  "browserify": {
    "transform": [
      "brfs",
      "browserify-shim"
    ]
  },
  "dependencies": {
    "bows": "^1.4.8",
    "jquery": "1.11.3",
    "underscore": "~1.8.3",
    "backbone": "1.3.2",
    "handlebars": "^4.0.5",
    "brfs": "^1.4.3"
  },
  "devDependencies": {
    "jsdom": "^8.1.0",
    "mocha": "^2.4.5"
  },
  "repository": "https://github.com/gadventures/backbone-forms",
  "author": "G Adventures",
  "license": "MIT",
  "readmeFilename": "README.md"
}
